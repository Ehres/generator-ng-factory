{
  "name": "{{ props.name }}",
  "version": "{{ props.version }}",
  "description": "{{ props.description }}",
  "keywords": [
    "angular"
  ],
{%- if props.username %}
  "homepage": "https://github.com/{{ props.username }}/{{ props.dashName }}",
  "bugs": "https://github.com/{{ props.username }}/{{ props.dashName }}/issues",
  "author": {
    "name": "{{ props.github.name }}",
    "email": "{{ props.github.email }}",
    "url": "https://github.com/{{ props.username }}"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/{{ props.username }}/{{ props.dashName }}.git"
  },
{%- endif %}
{%- if props.license %}
   "licenses": [
    {
      "type": "{{ props.license }}"
    }
  ],
{%- endif %}
  "dependencies": {
    {%- for component in props.components %}
    "{{ component|split("/")|last }}": "{{ components[component] }}",
    {%- endfor %}
    {%- for module in props.ngModules %}
    "angular-{{ module }}": "{{ props.ngVersion }}",
    {%- endfor %}
    "angular": "{{ props.ngVersion }}"
  }
}
