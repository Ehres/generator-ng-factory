{
  "name": "{{ props.pkgName }}",
  "version": "{{ props.version }}",
  "description": "{{ props.description }}",
  "keywords": [
    "angular"
  ],
{%- if props.username %}
  "homepage": "https://github.com/{{ props.username }}/{{ props.pkgName }}",
  "bugs": "https://github.com/{{ props.username }}/{{ props.pkgName }}/issues",
  "author": {
    "name": "{{ props.git.name }}",
    "email": "{{ props.git.email }}",
    "url": "https://github.com/{{ props.username }}"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/{{ props.username }}/{{ props.pkgName }}.git"
  },
{%- endif %}
{%- if props.license %}
   "licenses": [
    {
      "type": "{{ props.license }}"
    }
  ],
{%- endif %}
  "dependencies": {},
  "devDependencies": {
    "gulp": "git://github.com/gulpjs/gulp.git#4.0",
    "ng-factory": "^1.0",
    "undertaker-app-tasks": "^0.2",
    "factory-angular-channels": "^0.6"
  }
}
