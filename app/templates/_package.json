{
  "name": "{{ props.name }}",
  "version": "{{ props.version }}",
  "description": "{{ props.description }}",
  "keywords": [
    "angular"
  ],
{%- if props.github %}
  "homepage": "{{ props.github.html_url }}/{{ props.dashName }}",
  "bugs": "{{ props.github.html_url }}/{{ props.dashName }}/issues",
  "author": {
    "name": "{{ props.github.name }}",
    "email": "{{ props.github.email }}",
    "url": "{{ props.github.html_url }}"
  },
  "repository": {
    "type": "git",
    "url": "{{ props.github.html_url }}/{{ props.dashName }}.git"
  },
{%- endif %}
{%- if props.license %}
   "licenses": [
    {
      "type": "{{ props.license }}"
    }
  ],
{%- endif %}
  "dependencies": {},
  "devDependencies": {
    "chalk": "^0.5.1",
    "gulp": "^3.8.8",
    "gulp-autoprefixer": "^1.0.1",
    "gulp-changed": "^1.0.0",
    "gulp-clean": "^0.3.1",
    "gulp-cleancss": "^0.2.2",
    "gulp-concat-util": "^0.4.0",
    "gulp-connect": "^2.0.6",
    "gulp-htmlmin": "^0.2.0",
{%- if props.htmlPreprocessor == 'jade' %}
    "gulp-jade": "^0.8.0",
{%- endif %}
    "gulp-jshint": "^1.8.4",
    "gulp-karma": "^0.0.4",
{%- if props.htmlPreprocessor == 'less' %}
    "gulp-less": "^1.3.6",
{%- endif %}
    "gulp-ng-annotate": "^0.3.3",
    "gulp-nginclude": "^0.4.5",
    "gulp-ngtemplate": "^0.2.3",
    "gulp-open": "^0.2.8",
    "gulp-rename": "^1.2.0",
    "gulp-rimraf": "^0.1.0",
    "gulp-sourcemaps": "^1.2.2",
    "gulp-uglify": "^1.0.1",
    "gulp-usemin": "^0.3.8",
    "gulp-util": "^3.0.1",
    "gulp-watch": "^1.0.7",
    "jshint-stylish": "^1.0.0",
    "merge-stream": "^0.1.6",
    "run-sequence": "^0.3.7",
    "through2": "^0.6.2",
    "wiredep": "^1.8.5"
  }
}
